{
  "extends": "./.nuxt/tsconfig.json",
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ],
  "compilerOptions": {
    "target": "ES2018",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "importHelpers": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@pinia/nuxt"
    ],
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "isolatedModules": true,
    "verbatimModuleSyntax": true
  }
}
